@model List<MonksInn.Domain.File>


    <div class="highlight">
            @foreach (var item in Model)
            {
                <div class="card" style="width:200px;">
                    <div class="card-body">
                        <img src="@Url.Action("File", "Get", new { id=item.Id})" />
                    </div>
                </div>
            }
    </div>